{
    "name": "fsdb",
    "header": "fsdb_node.h",
    "init": "fsdb_node_init",
    "cleanup": "fsdb_node_cleanup",
    "com": [
        {
            "type": "service",
            "topic": "db",
            "resources": [{"name": "session",
                           "mimetype_out": "text/html",
                           "onrequest": "fsdb_get_session"},
                          {"name": "*",
                           "mimetype_out": "text/html",
                           "onrequest": "fsdb_get"}]
        },
        {
            "type": "messagehub",
            "topic": "db",
            "onmessage": "fsdb_onmessage"
        }
    ]
}
